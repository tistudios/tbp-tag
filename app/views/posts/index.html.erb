<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Hosting</th>
      <th>Background</th>
      <th>Age</th>
      <th>Can travel</th>
      <th>Category</th>
      <th>Location</th>
      <th>Posted</th>
      <th>Body type</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.hosting %></td>
        <td><%= post.background %></td>
        <td><%= post.age %></td>
        <td><%= post.can_travel %></td>
        <td><%= post.category %></td>
        <td><%= post.location %></td>
        <td><%= post.posted %></td>
        <td><%= post.body_type %></td>
        <td><%= post.content %></td>
        <td><%= post.user_id %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
